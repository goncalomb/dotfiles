[alias]
	d = diff
	s = status
	c = commit
	ca = commit -a
	a = commit --amend
	aa = commit -a --amend
	ane = commit --amend --no-edit
	aane = commit -a --amend --no-edit

	lof = log --format=fuller --decorate
	lol = log --graph --decorate --pretty=oneline --abbrev-commit --all

	who = shortlog -nse
	whoami = config --get-regexp \"^user\\.\"
[color]
	ui = auto
[push]
	default = upstream
